#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/typescript@4.9.5/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/typescript@4.9.5/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/typescript@4.9.5/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../.pnpm/typescript@4.9.5/node_modules/typescript/bin/tsserver" "$@"
fi
