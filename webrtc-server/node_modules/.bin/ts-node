#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/ts-node@10.9.2_@types+node@18.19.130_typescript@4.9.5/node_modules/ts-node/dist/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/ts-node@10.9.2_@types+node@18.19.130_typescript@4.9.5/node_modules/ts-node/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/ts-node@10.9.2_@types+node@18.19.130_typescript@4.9.5/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/ts-node@10.9.2_@types+node@18.19.130_typescript@4.9.5/node_modules/ts-node/dist/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/ts-node@10.9.2_@types+node@18.19.130_typescript@4.9.5/node_modules/ts-node/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/ts-node@10.9.2_@types+node@18.19.130_typescript@4.9.5/node_modules:/home/nginze/Documents/streams/webrtc-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin.js" "$@"
fi
